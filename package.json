{
  "name": "ts-playground",
  "version": "1.0.0",
  "description": "Typescript playground",
  "main": "src/main.ts",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf ./dist ./node_modules",
    "fetch": "ts-node --files ./scripts/clone-repo.ts",
    "fetch:jquery": "yarn fetch --url https://github.com/jquery/jquery.git --package-name jquery",
    "format": "prettier --write .",
    "lint": "prettier --check .",
    "start": "ts-node -p . --files",
    "start:prod": "node ./dist/main.js",
    "test": "ts-node ./tests/main.ts",
    "test:types": "yarn test types",
    "test:runtime": "yarn test utils",
    "watch": "tsc -w"
  },
  "dependencies": {
    "@babel/core": "^7.12.16",
    "@babel/parser": "^7.12.11",
    "axios": "^0.21.1",
    "babel-core": "^6.26.3",
    "isomorphic-git": "^1.8.0",
    "jquery": "^3.5.1",
    "jsdom": "^16.4.0"
  },
  "devDependencies": {
    "@babel/types": "^7.12.12",
    "@types/babel-types": "^7.0.9",
    "@types/babel__core": "^7.1.12",
    "@types/jquery": "^3.5.5",
    "@types/jsdom": "^16.2.6",
    "@types/node": "^14.14.20",
    "@typescript-eslint/eslint-plugin": "^4.12.0",
    "eslint": "^7.17.0",
    "eslint-config-prettier": "^7.1.0",
    "prettier": "^2.2.1",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  }
}
